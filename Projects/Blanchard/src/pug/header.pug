      .header__bottom 
        .container
          nav.nav.header__bottom-nav.bottom-nav
            ul.nav__list.bottom-nav__list
              each genre, i in listOfGenre
                li.nav__item.bottom-nav__item
                  a.bottom-nav__item-title.link.link_bg(href="#") #{genre}
                  .bottom-nav__sublist-wrapper
                    ul.bottom-nav__sublist
                      each style, j in listOfStyles[i]
                        li.bottom-nav__sublist-item 
                          a.nav__link(class="nav__link_" + style, href="#") #{listOfPeople[i][j]}
