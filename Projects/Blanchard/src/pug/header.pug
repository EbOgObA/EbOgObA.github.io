    .container
      a.logo.header__logo(href="#" aria-label="Логотип компании")
        img.logo__img(src="@img/icons/logo.svg" alt="Logo")
      .menu.header__menu 
        button.icon-menu.burger.btn-reset(aria-label="Открыть меню")
          span.burger__line 
          span.burger__line 
          span.burger__line
        nav.nav.menu__body(title="Основное меню")
          ul.nav__list.menu__list 
            li.nav__item.menu__item
              a.nav__link.link.link_bg.goto(href="#about") О&nbsp;нас
            li.nav__item.menu__item
              a.nav__link.link.link_bg.goto(href="#gallery") Галерея
            li.nav__item.menu__item
              a.nav__link.link.link_bg.goto(href="#catalog") Каталог
            li.nav__item.menu__item
              a.nav__link.link.link_bg.goto(href="#events") События
            li.nav__item.menu__item
